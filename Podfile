platform :ios, '10.0'

def pods
    use_frameworks!
    
    pod 'Alamofire'
    pod 'ObjectMapper', '~> 3.1'
    pod 'SnapKit', '~> 4.0.0'
    pod 'GooglePlaces'
    pod 'GooglePlacePicker'
    pod 'GoogleMaps'
end

target 'Fase_iOS_notes' do
    pods
end

target 'Fase_iOS_karma' do
    pods
end

target 'Fase_iOS_test' do
    pods
end

